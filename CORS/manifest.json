{
  "manifest_version": 2,
  "name": "CORS Changer",
  "description": "Allows requests to other domains by adding 'Allow-origin: *' to headers. Some sites may not work as expected with this extension on.",
  "version": "1.0.0",
  "permissions": [
    "storage", "webRequest", "webRequestBlocking", "<all_urls>"
    ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "/popup/popup.html"
  },
  "web_accessible_resources": []
}
